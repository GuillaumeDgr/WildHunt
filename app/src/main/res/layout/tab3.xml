<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="fr.indianacroft.wildhunt.Tab_Activity$PlaceholderFragment">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintRight_toLeftOf="@+id/partyName"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintBottom_toTopOf="@+id/butValidateAll"
        />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintRight_toLeftOf="@+id/textView14"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Party\nName"
        app:layout_constraintRight_toLeftOf="@+id/textView15"
        app:layout_constraintLeft_toRightOf="@+id/imageView6"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Numbers of\nPlayers"
        app:layout_constraintRight_toLeftOf="@+id/textView16"
        app:layout_constraintLeft_toRightOf="@+id/textView14"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pending\nChallenges"
        app:layout_constraintRight_toLeftOf="@+id/button8"
        app:layout_constraintLeft_toRightOf="@+id/textView15"
        app:layout_constraintBottom_toTopOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Check\nValidations"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView16"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/button9" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintRight_toLeftOf="@+id/textView17"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Party\nName"
        app:layout_constraintRight_toLeftOf="@+id/textView18"
        app:layout_constraintLeft_toRightOf="@+id/imageView7"
        app:layout_constraintBottom_toTopOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Numbers of\nPlayers"
        app:layout_constraintRight_toLeftOf="@+id/textView19"
        app:layout_constraintLeft_toRightOf="@+id/textView17"
        app:layout_constraintBottom_toTopOf="@+id/textView15"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pending\nChallenges"
        app:layout_constraintRight_toLeftOf="@+id/button9"
        app:layout_constraintLeft_toRightOf="@+id/textView18"
        app:layout_constraintBottom_toTopOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Check\nValidations"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView19"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintRight_toLeftOf="@+id/textView11"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Party\nName"
        app:layout_constraintRight_toLeftOf="@+id/textView12"
        app:layout_constraintLeft_toRightOf="@+id/imageView5"
        app:layout_constraintBottom_toTopOf="@+id/partyName"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Numbers of\nPlayers"
        app:layout_constraintRight_toLeftOf="@+id/textView13"
        app:layout_constraintLeft_toRightOf="@+id/textView11"
        app:layout_constraintBottom_toTopOf="@+id/players"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pending\nChallenges"
        app:layout_constraintRight_toLeftOf="@+id/button7"
        app:layout_constraintLeft_toRightOf="@+id/textView12"
        app:layout_constraintBottom_toTopOf="@+id/nbChallenges"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Check\nValidations"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView13"
        app:layout_constraintBottom_toTopOf="@+id/butValidate"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <TextView
        android:id="@+id/partyName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Party\nName"
        app:layout_constraintRight_toLeftOf="@+id/players"
        app:layout_constraintLeft_toRightOf="@+id/imageView3"
        app:layout_constraintBottom_toTopOf="@+id/butValidateAll"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/players"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Numbers of\nPlayers"
        app:layout_constraintRight_toLeftOf="@+id/nbChallenges"
        app:layout_constraintLeft_toRightOf="@+id/partyName"
        app:layout_constraintBottom_toTopOf="@+id/butValidateAll"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/nbChallenges"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pending\nChallenges"
        app:layout_constraintRight_toLeftOf="@+id/butValidate"
        app:layout_constraintLeft_toRightOf="@+id/players"
        app:layout_constraintBottom_toTopOf="@+id/butValidateAll"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <Button
        android:id="@+id/butValidate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Check\nValidations"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/nbChallenges"
        app:layout_constraintBottom_toTopOf="@+id/butValidateAll"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        />

    <Button
        android:id="@+id/butValidateAll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Validate all challenges"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/partyName"
        />
</android.support.constraint.ConstraintLayout>
